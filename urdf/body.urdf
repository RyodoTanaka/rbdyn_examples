<?xml version="1.0"?>
<robot name="tl01">

  <link name="world"/>

  <link name="base_link"/>
  <joint name="world_to_base_link" type="floating">
    <origin xyz="1.0 1.733 0"/>
    <parent link="world"/>
    <child link="base_link"/>
  </joint>

  <link name="leg1_link"/>
  <joint name="base_link_to_leg1_link" type="revolute">
    <axis xyz="0 0 1"/>
    <parent link="base_link"/>
    <child link="leg1_link"/>
    <limit effort="1.0" lower="-3.14" upper="3.14" velocity="3.14"/>
  </joint>

  <link name="leg1_end_link"/>
  <joint name="leg1_link_to_leg1_end_link" type="fixed">
    <origin xyz="0 -2.0 0"/>
    <parent link="leg1_link"/>
    <child link="leg1_end_link"/>
  </joint>

  <link name="leg2_link"/>
  <joint name="base_link_to_leg2_link" type="revolute">
    <axis xyz="0 0 1"/>
    <parent link="base_link"/>
    <child link="leg2_link"/>
    <limit effort="1.0" lower="-3.14" upper="3.14" velocity="3.14"/>
  </joint>

  <link name="leg2_end_link"/>
  <joint name="leg2_link_to_leg2_end_link" type="fixed">
    <origin xyz="0 -2.0 0"/>
    <parent link="leg2_link"/>
    <child link="leg2_end_link"/>
  </joint>
</robot>
